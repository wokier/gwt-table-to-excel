<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Servlets -->
	<servlet>
		<servlet-name>greetServlet</servlet-name>
		<servlet-class>com.googlecode.gwtTableToExcel.demo.server.rpc.GreetingServiceImpl</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>gwtTableToExcelServlet</servlet-name>
		<servlet-class>com.googlecode.gwtTableToExcel.server.TableToExcelServlet</servlet-class>
	</servlet>
<!--	<servlet>-->
<!--		<servlet-name>jUnitHostImpl</servlet-name>-->
<!--		<servlet-class>com.google.gwt.junit.server.JUnitHostImpl</servlet-class>-->
<!--	</servlet>-->

	<servlet-mapping>
		<servlet-name>greetServlet</servlet-name>
		<url-pattern>/gtte/greet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>gwtTableToExcelServlet</servlet-name>
		<url-pattern>/gtte/excel</url-pattern>
	</servlet-mapping>

<!--	<servlet-mapping>-->
<!--		<servlet-name>jUnitHostImpl</servlet-name>-->
<!--		<url-pattern>/gtte/junithost/*</url-pattern>-->
<!--	</servlet-mapping>-->
	
	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>
